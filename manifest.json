{
  "manifest_version": 2,
  "name": "GlobalTab",
  "description": "New tab page showing your favorite bookmarks.",
  "version": "1.0.0",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "author": "Miguel Nunes",
  "chrome_url_overrides": {
    "newtab": "globaltab.html"
  },
  "permissions": [
    "bookmarks",
    "management",
    "chrome://favicon/"
  ]
}
